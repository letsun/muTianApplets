/* home/downloadCenter/downloadCenter.wxss */

.swiper {
  height: 350rpx;
  width: 690rpx;
  margin: auto;
  border-radius: 15rpx;
  box-shadow: 0px 6px 15rpx 0rpx #e2e2e1;
}

.swiper-item {
  height: 350rpx;
  width: 690rpx;
  margin: auto;
  border-radius: 15rpx;
}

.swiper-item image {
  height: 350rpx;
  width: 690rpx;
}

.swiper .wx-swiper-dot-active::before {
  background: #df2c3f;
}

.swiper .wx-swiper-dot {
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
}

.swiper .wx-swiper-dot.wx-swiper-dot-active {
  width: 34rpx;
  height: 12rpx;
  border-radius: 8rpx;
}

.content {
  padding-top: 20rpx;
}
.proinfo {
  padding: 30rpx 30rpx 0 30rpx;
  border-bottom: 1rpx solid #eee;
  color: #666;
}

.proinfo-title {
  font-size: 30rpx;
  font-weight: bold;
}

.proinfo-title image {
  max-height: 25rpx;
  max-width: 23rpx;
}

.proinfo-con {
  position: relative;
  padding: 30rpx;
  display: flex;
}

.bo1 {
  position: absolute;
  right: 20rpx;
  top: 32rpx;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  margin: 0 10rpx 0 0rpx;
  border: 1px solid #ddd;
}

.bo2 {
  position: absolute;
  right: 20rpx;
  top: 32rpx;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  margin: 0 10rpx 0 0rpx;
  border: 1px solid #ddd;
  background-image: repeating-radial-gradient(#ddd 0px, #ddd 10rpx, #fff 10rpx, #fff 20rpx);
}



.bo3 {
  position: absolute;
  right: 20rpx;
  top: 32rpx;
  width: 16px;
  height: 16px;
  border-radius: 50%;
  margin: 0 10rpx 0 0rpx;
  border: 1px solid #fe6a00;
  background-image: repeating-radial-gradient(#fe6a00 0px, #fe6a00 10rpx, #fff 10rpx, #fff 20rpx);
}



.check {
  padding: 30rpx 30rpx 0 30rpx;
  color: #666;
}

.check-title {
  font-size: 30rpx;
  font-weight: bold;
}

.check-title image {
  max-height: 27rpx;
  max-width: 23rpx;
}

.check-title {
  position: relative;
}

.check-title-inp {
  position: absolute;
  top: 0rpx;
  right: 0;
  width: 300rpx;
}

.check-title-inp image {
  position: absolute;
  left: 0;
  top: 13rpx;
  height: 22rpx;
  width: 22rpx;
}

.check-title-inp input {
  position: absolute;
  right: 0;
  font-size: 22rpx;
  width: 270rpx;
  border-radius: 30rpx;
  padding-left: 50rpx;
  border: 1rpx solid #eee;
}

.check-con {
  padding: 30rpx 20rpx;
  line-height: 70rpx;
}

.check-con-header {
  display: flex;
  background: #f6f6f6;
}

.check-con-header-item {
  text-align: center;
}

.check-con-header-item:first-child {
  width: 60%;
}

.check-con-header-item:nth-child(2) {
  width: 20%;
}

.check-con-con {
  display: flex;
  border-bottom: 1rpx solid #eee;
}

.check-con-con:last-child {
  border: none;
}

.check-con-con-item {
  text-align: center;
}

.check-con-con-item:first-child {
  width: 60%;
}

.check-con-con-item:nth-child(2) {
  width: 20%;
}

.check-con-con-item:last-child {
  width: 20%;
  position: relative;
}

