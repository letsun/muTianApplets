.tfooter {
  height: 100rpx;
}

.footer {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100rpx;
  display: flex;
  box-shadow: 0px 0px 20rpx 0rpx #e2e2e1;
  background: #fff;
  z-index: 20;
}

.footer-dow {
  position: relative;
  width: 20%;
  text-align: center;
}

.footer-dow-num {
  position: absolute;
  left: 90rpx;
  top: 10rpx;

  border-radius: 50%;
  font-size: 16rpx;
  height: 35rpx;
  width: 35rpx;
  line-height: 35rpx;
  text-align: center;
  background: #e10010;
  color: #f4f4f4;
}

.footer-dow image {
  margin-top: 30rpx;
  width: 49rpx;
  height: 45rpx;
}

.footer-total {
  width: 45%;
  display: flex;
  margin-top: 40rpx;
}

.footer-total-texta {
  padding-right: 15rpx;
}

.footer-total-textb {
  font-weight: bold;
  color: #e21414;
}

.footer-total-textc {
  font-weight: bold;
  font-size: 45rpx;
  margin-top: -15rpx;
  color: #e21414;
}

.footer-godow {
  width: 35%;
  color: #f4f4f4;
  font-size: 32rpx;
  text-align: center;
  line-height: 100rpx;
  font-weight: bold;
  background: #ff6b10;
}

.mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.6);
}

.mask-con {
  position: fixed;
  bottom: 100rpx;
  left: 0;
  right: 0;
  z-index: 15;
  background: #fff;
}

.mask-con-title {
  padding: 15rpx 30rpx;
  background: #fffde8;
  color: #666;
  text-align: right;
}

.mask-con-title image {
  width: 26rpx;
  height: 28rpx;
  vertical-align: middle;
}

.mask-con-text {
  color: #666;
  font-size: 32rpx;
  padding: 20rpx 30rpx;
  margin-right: 5rpx;
}

.mask-con-text span {
  font-size: 24rpx;

}


.mask-con-con{
  overflow-x: hidden;
  height: 400rpx;
  font-size: 24rpx;
}

.mask-con-item {
  display: flex;

  padding: 30rpx;

  border-bottom: 1rpx solid #eee;
}

.mask-con-item-name {
  width: 23%;
}

.mask-con-item-batchNo {
  width: 35%;
}

.mask-con-item-left {
  width: 20%;
  text-align: center;
}

.mask-con-item-middle {
  width: 12%;
  font-weight: bold;
  color: #e21414;
}

.mask-con-item-right {
  width: 10%;
  text-align: center;
}


.mask-con-item-right image {
  width: 26rpx;
  height: 28rpx;
}